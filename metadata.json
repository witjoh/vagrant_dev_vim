{
  "name": "johan-vim",
  "version": "0.2.0",
  "author": "johan",
  "summary": "Configures a users vim to get a better code editor",
  "license": "Apache-2.0",
  "source": "http://gitlabee.koewacht.net/puppetcode/vim.git",
  "project_page": "http://gitlabee.koewacht.net/puppetcode/vim.git",
  "issues_url": "http://gitlabee.koewacht.net/puppetcode/vim.git",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "puppetlabs-vcsrepo",
      "version_requirement": ">= 1.3.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ]
}
